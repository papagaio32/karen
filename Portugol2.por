programa
{
	
	funcao inicio()
	{
		real salarioatual, novosalario
		escreva("informe seu salário ")
		leia(salarioatual)

		novosalario = salarioatual +(salarioatual*0.25)

		escreva("Seu novo salário é de R$" + novosalario)
		
	}
}
